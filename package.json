    "radium": "^0.13.8"
  },
  "peerDependencies": {
    "react": "^0.14.0-beta3",
    "react-dom": "^0.14.0-beta3"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
